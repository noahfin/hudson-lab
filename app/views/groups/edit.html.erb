<div id="main-wrap">
   <div class="container">
      <div class="col-md-6 mr-auto ml-auto p-4">
         <div class="row ">
            <div class="col-md-8">
               <h1>Editing Group</h1>
            </div>
            <div class="col-md-1 "> </div>
            <div class="col-md-3 mt-4 pl-4">
               <%= link_to 'Show', @group, class: 'text-info' %> | <%= link_to 'Back', groups_path, class: 'text-info' %>
            </div>
         </div>
         <%= render 'form', group: @group %>
         <button type="submit" class="btn btn-block bg-black  mt-4">Update Group</button>
      </div>
   </div>
</div>
